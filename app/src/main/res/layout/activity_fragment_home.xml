<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center"
    android:orientation="vertical"
    android:background="#408ad4"
    android:weightSum="2">
    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="@dimen/_20sdp"
        android:id="@+id/relativeLayout">
        <ScrollView
            android:layout_width="wrap_content"
            android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:weightSum="2">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical"
                android:weightSum="8">
                <RelativeLayout
                    android:id="@+id/layUser"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:background="@drawable/drawable_left_nine">
                    <ImageView
                        android:id="@+id/imgUserProfile"
                        android:layout_width="@dimen/_80sdp"
                        android:layout_height="@dimen/_70sdp"
                        android:layout_centerHorizontal="true"
                        android:paddingTop="@dimen/_5sdp"
                        android:src="@drawable/ic_userprofile" />
                    <TextView
                        android:id="@+id/txtUserProfile"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/imgUserProfile"
                        android:layout_centerHorizontal="true"
                        android:text="@string/my_profile"
                        android:textColor="@color/white_color"
                        android:textSize="@dimen/_13sdp" />
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_1sdp"
                        android:layout_below="@+id/txtUserProfile"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:background="#A9D3FF" />
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/layResidency"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:background="@drawable/drawable_left_nine">
                    <ImageView
                        android:id="@+id/imageResidency"
                        android:layout_width="@dimen/_180sdp"
                        android:layout_height="@dimen/_70sdp"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentTop="true"
                        android:paddingTop="@dimen/_5sdp"
                        android:src="@drawable/ic_recidency" />
                    <TextView
                        android:id="@+id/txtResidency"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/imageResidency"
                        android:layout_centerHorizontal="true"
                        android:text="@string/residency_directory"
                        android:textColor="@color/white_color"
                        android:textSize="@dimen/_13sdp" />
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_1sdp"
                        android:layout_below="@+id/txtResidency"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:background="#A9D3FF" />
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/layComplaint"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:background="@drawable/drawable_left_nine">

                    <ImageView
                        android:id="@+id/imagecomplaint"
                        android:layout_width="@dimen/_80sdp"
                        android:layout_height="@dimen/_70sdp"
                      android:layout_centerHorizontal="true"
                        android:paddingTop="@dimen/_5sdp"
                        android:src="@drawable/ic_complain" />
                    <TextView
                        android:id="@+id/txtComplaint"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/imagecomplaint"
                        android:layout_centerHorizontal="true"
                        android:text="@string/complaint"
                        android:textColor="@color/white_color"
                        android:textSize="@dimen/_13sdp" />
                </RelativeLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_1sdp"
                    android:layout_above="@+id/layNotice"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:background="#A9D3FF" />
                <RelativeLayout
                    android:id="@+id/layNotice"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:background="@drawable/drawable_left_nine">
                    <ImageView
                        android:id="@+id/imageNotice"
                        android:layout_width="@dimen/_180sdp"
                        android:layout_height="@dimen/_70sdp"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentTop="true"
                        android:paddingTop="@dimen/_5sdp"
                        android:src="@drawable/ic_notice" />
                    <TextView
                        android:id="@+id/txtNotice"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/imageNotice"
                        android:layout_centerHorizontal="true"
                        android:text="@string/notice"
                        android:textColor="@color/white_color"
                        android:textSize="@dimen/_13sdp" />
                </RelativeLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_1sdp"
                    android:layout_above="@+id/layBilling"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:background="#A9D3FF" />
                <RelativeLayout
                    android:id="@+id/layBilling"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:background="@drawable/drawable_left_nine">
                    <ImageView
                        android:id="@+id/imageBilling"
                        android:layout_width="@dimen/_180sdp"
                        android:layout_height="@dimen/_70sdp"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentTop="true"
                        android:paddingTop="@dimen/_5sdp"
                        android:src="@drawable/ic_billing" />
                    <TextView
                        android:id="@+id/txtBilling"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/imageBilling"
                        android:layout_centerHorizontal="true"
                        android:text="@string/billing"
                        android:textColor="@color/white_color"
                        android:textSize="@dimen/_13sdp" />
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_1sdp"
                        android:layout_below="@+id/txtBilling"
                        android:background="#A9D3FF" />
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/layVehicle"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:background="@drawable/drawable_left_nine">
                    <ImageView
                        android:id="@+id/imageVechile"
                        android:layout_width="@dimen/_180sdp"
                        android:layout_height="@dimen/_70sdp"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentTop="true"
                        android:paddingTop="@dimen/_5sdp"
                        android:src="@drawable/ic_vechile" />
                    <TextView
                        android:id="@+id/txtVechile"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/imageVechile"
                        android:layout_centerHorizontal="true"
                        android:text="@string/vehicle"
                        android:textColor="@color/white_color"
                        android:textSize="@dimen/_13sdp" />
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_1sdp"
                        android:layout_below="@+id/txtVechile"
                        android:background="#A9D3FF" />
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/layAttandance"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:background="@drawable/drawable_left_nine">
                    <ImageView
                        android:id="@+id/imageAttandance"
                        android:layout_width="@dimen/_180sdp"
                        android:layout_height="@dimen/_70sdp"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentTop="true"
                        android:paddingTop="@dimen/_5sdp"
                        android:src="@drawable/ic_attendance" />
                    <TextView
                        android:id="@+id/txtAttandance"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/imageAttandance"
                        android:layout_centerHorizontal="true"
                        android:text="@string/attendance"
                        android:textColor="@color/white_color"
                        android:textSize="@dimen/_13sdp" />
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_1sdp"
                        android:layout_below="@+id/txtAttandance"
                        android:background="#A9D3FF" />
                </RelativeLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical"
                android:weightSum="7">
                <RelativeLayout
                    android:id="@+id/layRequest"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/drawable_left_nine"
                    android:layout_gravity="center"
                    android:layout_weight="1">
                    <ImageView
                        android:id="@+id/request"
                        android:layout_width="@dimen/_80sdp"
                        android:layout_height="@dimen/_70sdp"
                        android:layout_centerHorizontal="true"
                        android:src="@drawable/ic_request" />
                    <TextView
                        android:id="@+id/txtRequest"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/request"
                        android:layout_centerHorizontal="true"
                        android:text="@string/make_request"
                        android:textColor="@color/white_color"
                        android:textSize="@dimen/_13sdp" />
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_1sdp"
                        android:layout_below="@+id/txtRequest"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:background="#A9D3FF" />
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/layParking"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:background="@drawable/drawable_left_nine">
                    <ImageView
                        android:id="@+id/imgParking"
                        android:layout_width="@dimen/_80sdp"
                        android:layout_height="@dimen/_70sdp"
                        android:paddingTop="@dimen/_5sdp"
                        android:layout_centerHorizontal="true"
                        android:src="@drawable/ic_parking" />
                    <TextView
                        android:id="@+id/txtParking"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/imgParking"
                        android:layout_centerHorizontal="true"
                        android:text="Parking"
                        android:textColor="@color/white_color"
                        android:textSize="@dimen/_13sdp" />
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_1sdp"
                        android:layout_below="@+id/txtParking"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:background="#A9D3FF" />
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/layMeeting"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:background="@drawable/drawable_left_nine">
                    <ImageView
                        android:id="@+id/imgMeeting"
                        android:layout_width="@dimen/_80sdp"
                        android:layout_height="@dimen/_70sdp"
                        android:paddingTop="@dimen/_5sdp"
                        android:layout_centerHorizontal="true"
                        android:src="@drawable/ic_meeting" />
                    <TextView
                        android:id="@+id/txtMeeting"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/imgMeeting"
                        android:layout_centerHorizontal="true"
                        android:text="@string/meeting"
                        android:textColor="@color/white_color"
                        android:textSize="@dimen/_13sdp" />
                </RelativeLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_1sdp"
                    android:layout_above="@+id/layAMCservice"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:background="#A9D3FF" />
                <RelativeLayout
                    android:id="@+id/layAMCservice"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:background="@drawable/drawable_left_nine">
                    <ImageView
                        android:id="@+id/imgAmcService"
                        android:layout_width="@dimen/_80sdp"
                        android:layout_height="@dimen/_70sdp"
                        android:paddingTop="@dimen/_5sdp"
                        android:layout_centerHorizontal="true"
                        android:src="@drawable/ic_amc" />
                    <TextView
                        android:id="@+id/txtAmcservices"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/imgAmcService"
                        android:layout_centerHorizontal="true"
                        android:text="@string/amc_services"
                        android:textColor="@color/white_color"
                        android:textSize="@dimen/_13sdp" />
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_1sdp"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:layout_below="@+id/txtAmcservices"
                        android:background="#A9D3FF" />
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/layFacility"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:background="@drawable/drawable_left_nine">
                    <ImageView
                        android:id="@+id/imgFacility"
                        android:layout_width="@dimen/_80sdp"
                        android:layout_height="@dimen/_70sdp"
                        android:paddingTop="@dimen/_5sdp"
                        android:layout_centerHorizontal="true"
                        android:src="@drawable/ic_facility" />
                    <TextView
                        android:id="@+id/txtFacility"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/imgFacility"
                        android:layout_centerHorizontal="true"
                        android:text="@string/facility_mgmt"
                        android:textColor="@color/white_color"
                        android:textSize="@dimen/_13sdp" />
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_1sdp"
                        android:layout_below="@+id/txtFacility"
                        android:background="#A9D3FF" />
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/layVisitor"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:background="@drawable/drawable_left_nine">
                    <ImageView
                        android:id="@+id/imgVisitor"
                        android:layout_width="@dimen/_80sdp"
                        android:layout_height="@dimen/_70sdp"
                        android:paddingTop="@dimen/_5sdp"
                        android:layout_centerHorizontal="true"
                        android:src="@drawable/ic_visitor" />
                    <TextView
                        android:id="@+id/txtVisitors"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/imgVisitor"
                        android:layout_centerHorizontal="true"
                        android:text="@string/visitor"
                        android:textColor="@color/white_color"
                        android:textSize="@dimen/_13sdp" />
                        <View
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_1sdp"
                        android:layout_below="@+id/txtVisitors"
                        android:background="#A9D3FF" />
                </RelativeLayout>
            </LinearLayout>
        </LinearLayout>
        </ScrollView>
    </RelativeLayout>
</RelativeLayout>

